{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": [
            "single"
        ],
        "member-ordering": [
            false
        ],
        "no-bitwise": false,
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "max-line-length": [
            false
        ],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-string-literal": false,
        "trailing-comma": [
            false
        ]
    }
}